function make_count()
{
    var long     = parseInt(document.getElementsByName('long')[0].value);
    var width    = parseInt(document.getElementsByName('width')[0].value);
    var height    = parseInt(document.getElementsByName('height')[0].value);
    var number   = parseInt(document.getElementsByName('number')[0].value);
    var diff     = document.getElementsByName('diff')[0].value;
    var material = document.getElementsByName('material')[0].value;
    var thick    = document.getElementsByName('thick')[0].value;

    if( !long || isNaN(long) ) { alert('Введите длину детали (число)'); return false; }
    if( !width || isNaN(width) ) { alert('Введите ширину детали (число)'); return false; }
    if( !height || isNaN(height) ) { alert('Введите высоту детали (число)'); return false; }
    if( !number || isNaN(number) ) { alert('Введите количество (число)'); return false; }

    var stoimost_materialov_za_1m_kvadr_zad_tolsh = 1.06 * thick * material;
    var stoimost_zagotovki = (((long+height+50)*(width+height+50))/1000000)*stoimost_materialov_za_1m_kvadr_zad_tolsh;
    if(diff == 1){
    	var stoimost_form = ((long*width*height)*600)/3150000;
    }else if(diff == 2){
    	var stoimost_form = ((long*width*height)*800)/3150000;
    }else if(diff == 3){
    	var stoimost_form = ((long*width*height)*1000)/3150000;
    }else{
    	var stoimost_form = 0;
    }
    if(number &lt; 100){
    	var summa = Math.ceil((stoimost_zagotovki*1.75*number) + stoimost_form);
    }else{
    	var summa = Math.ceil((stoimost_zagotovki*1.45*number) + stoimost_form);
    }
    var cena_shtuka = Math.ceil(summa/number);
    write_result(cena_shtuka,summa);
}

function write_result( str1, str2 )
{
    document.getElementById('res_weight').innerHTML = str1;
    document.getElementById('res_price').innerHTML = str2;
}
$(document).ready(function(){
	$(&quot;a.plusminus&quot;).click(function(e){
		e.preventDefault();
		if($(this).hasClass(&quot;minus_btn&quot;)){
			var value = parseInt($(this).next().val());
			if(value &gt; 1){
				$(this).next().val(value-1);
			}
		}else{
			var value = parseInt($(this).prev().val());
			$(this).prev().val(value+1);
		}
	});
});
